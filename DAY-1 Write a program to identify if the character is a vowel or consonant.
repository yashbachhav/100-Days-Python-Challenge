user_input=input("Enter alphabet: ")

vowel=str(["a","e","i","o","u"])
consonants= str(["b","c","d","f","g","h","j","k","l","m","n","p","q","r","s","t","v","w","x","y","z"])

if user_input in vowel:
    print("Vowel")
elif user_input in consonants:
    print("Consonants")
else:
    print("Invalid")
